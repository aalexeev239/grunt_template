{{#parseJSON '{"btns":["primary","secondary"]}'}}
  <div class="grid">
  {{#each btns}}
    <div class="grid__item one-quarter">
      <h2>{{this}}</h2>
      <br>
      <a href="##" class="btn btn--{{this}}" title="{{this}} кнопка">{{this}} кнопка</a>
      <br><br>
      <a href="##" class="btn btn--{{this}}" disabled title="{{this}} disabled">{{this}} disabled</a>
    </div>
  {{/each}}
  </div>
{{/parseJSON}}